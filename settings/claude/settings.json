{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "BASH_DEFAULT_TIMEOUT_MS": "30000",
    "CLAUDE_BASH_MAINTAIN_PROJECT_WORKING_DIR": "1",
    "DISABLE_TELEMETRY": "1",
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "40000",
    "MAX_MCP_OUTPUT_TOKENS": "40000",
    "MAX_THINKING_TOKENS": "10000",
    "SLASH_COMMAND_TOOL_CHAR_BUDGET": "30000",
    "ENABLE_TOOL_SEARCH": "auto:0"
  },
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "permissions": {
    "allow": [
      "__ READ-ONLY CLI __",
      "Bash(cat:*)",
      "Bash(difft:*)",
      "Bash(head:*)",
      "Bash(ls:*)",
      "Bash(printf:*)",
      "Bash(readlink:*)",
      "Bash(tail:*)",
      "Bash(test:*)",
      "Bash(tree:*)",
      "Bash(wc:*)",
      "Read(//Users/jacob.hurlburt/**)",
      "__ SEARCH __",
      "Bash(ast-grep:*)",
      "Bash(fd:*)",
      "Bash(fzf:*)",
      "Bash(grep:*)",
      "Bash(rg:*)",
      "__ DATA PROCESSING __",
      "Bash(jq:*)",
      "Bash(yq:*)",
      "__ GIT READ-ONLY __",
      "Bash(git blame:*)",
      "Bash(git worktree add:*)",
      "Bash(git worktree list:*)",
      "Bash(git worktree lock:*)",
      "Bash(git worktree unlock:*)",
      "Bash(git worktree repair:*)",
      "Bash(git worktree prune:*)",
      "Bash(git describe:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git ls-files:*)",
      "Bash(git ls-tree:*)",
      "Bash(git remote:*)",
      "Bash(git rev-parse:*)",
      "Bash(git shortlog:*)",
      "Bash(git show:*)",
      "Bash(git stash list:*)",
      "Bash(git status:*)",
      "__ GITHUB CLI READ-ONLY __",
      "Bash(gh * list *)",
      "Bash(gh * view *)",
      "Bash(gh * status *)",
      "Bash(gh * diff *)",
      "Bash(gh * checks *)",
      "__ LINTING/CHECKING __",
      "Bash(* pytest *)",
      "Bash(* ruff *)",
      "Bash(* ty *)",
      "Bash(pre-commit run:*)",
      "Bash(prek run:*)",
      "Bash(*sqruff*)",
      "__ UNIVERSAL FLAGS __",
      "Bash(* --help)",
      "Bash(* -h)",
      "Bash(* --version)",
      "Bash(* -V)",
      "__ WEB __",
      "WebFetch(domain:*)",
      "__ DATABRICKS CLI (READ-ONLY, ALL WORKSPACES) __",
      "Bash(databricks * list*)",
      "Bash(databricks * get*)",
      "Bash(databricks * describe*)",
      "Bash(databricks * export*)",
      "Bash(databricks auth *)",
      "__ PYTHON PACKAGE MANAGERS (READ-ONLY) __",
      "Bash(* pip index *)",
      "Bash(* pip list *)",
      "Bash(* pip show *)",
      "Bash(* pip check *)",
      "Bash(* pip freeze *)",
      "Bash(* pip config list *)",
      "Bash(uv cache dir *)",
      "Bash(uv tool list *)",
      "Bash(uv python list *)",
      "Bash(uv version *)",
      "Bash(uv self version *)",
      "__ MCP TOOLS __",
      "mcp__databricks-dbsql__execute_sql_read_only",
      "mcp__databricks-dbsql__poll_sql_result",
      "mcp__plugin_atlassian-toolkit_atlassian-mcp",
      "mcp__plugin_episodic-memory_episodic-memory",
      "mcp__private-journal",
      "mcp__ide",
      "__ SKILLS __",
      "Skill(*)"
    ],
    "ask": ["Bash(databricks auth token*)"],
    "deny": [
      "__ DATABRICKS CLI DESTRUCTIVE __",
      "mcp__databricks-dbsql__execute_sql",
      "__ GIT DESTRUCTIVE __",
      "Bash(git clean -f:*)",
      "Bash(git commit --no-verify:*)",
      "Bash(git commit -n:*)",
      "Bash(git push --force:*)",
      "Bash(git push -f:*)",
      "Bash(git reset --hard:*)",
      "__ PRE-COMMIT BYPASS __",
      "Bash(SKIP=:*)",
      "Bash(pre-commit uninstall:*)",
      "Bash(prek uninstall:*)",
      "__ SECRETS __",
      "Read(**/.env*)",
      "Read(~/.aws/**)",
      "Read(~/.config/gcloud/**)",
      "Read(~/.ssh/**)"
    ],
    "defaultMode": "acceptEdits",
    "disableBypassPermissionsMode": "disable"
  },
  "model": "opus",
  "enableAllProjectMcpServers": true,
  "statusLine": {
    "type": "command",
    "command": "input=$(cat); printf \"\\033[2m%s@%s \\033[36m%s\\033[0m \\033[2m[%s] \\033[33m%s%%\\033[0m\" \"$(whoami)\" \"$(hostname -s)\" \"$(echo \"$input\" | jq -r '.workspace.current_dir | gsub(\".*/\"; \"\")' 2>/dev/null || basename \"$(echo \"$input\" | jq -r '.workspace.current_dir')\" 2>/dev/null || echo \"unknown\")\" \"$(echo \"$input\" | jq -r '.model.display_name' 2>/dev/null || echo \"Claude\")\" \"$(echo \"$input\" | jq -r '.context_window.used_percentage' 2>/dev/null || echo \"0\")\""
  },
  "enabledPlugins": {
    "superpowers@superpowers-marketplace": true,
    "episodic-memory@superpowers-marketplace": true,
    "superpowers-lab@superpowers-marketplace": true,
    "atlassian-toolkit@kin-claude-plugins": true,
    "code-review@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "hookify@claude-plugins-official": true,
    "plugin-dev@claude-plugins-official": true,
    "pr-review-toolkit@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": true,
    "security-guidance@claude-plugins-official": true,
    "superpowers-developing-for-claude-code@superpowers-marketplace": true,
    "planning-with-files@planning-with-files": true,
    "kin-design-system@kin-claude-plugins": true,
    "astral@astral-sh": true,
    "feature-dev@claude-plugins-official": true,
    "agent-sdk-dev@claude-plugins-official": true,
    "claude-md-management@claude-plugins-official": true,
    "claude-code-setup@claude-plugins-official": true,
    "terraform-ls@kin-claude-plugins": true
  },
  "extraKnownMarketplaces": {
    "superpowers-marketplace": {
      "source": {
        "source": "github",
        "repo": "obra/superpowers-marketplace"
      }
    },
    "kin-claude-plugins": {
      "source": {
        "source": "directory",
        "path": "/Users/jacob.hurlburt/repos/claude-plugins"
      }
    },
    "astral-sh": {
      "source": {
        "source": "github",
        "repo": "astral-sh/claude-code-plugins"
      }
    }
  }
}
